def output_radius
array ball_rad(50000)
ii = 0
bp = ball_head
loop while bp # null
	ii = ii + 1
	temp_radius = b_rad(bp)
	ball_rad(ii) = string(temp_radius)
	bp = b_next(bp)
end_loop

status=open('ball_rad.txt', 1, 1)
status=write(ball_rad, ii )
status=close

end